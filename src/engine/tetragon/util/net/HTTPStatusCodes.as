K",			"201": "Created",			"202": "Accepted",			"203": "Non-Authoritative Information",			"204": "No Content",			"205": "Reset Content",			"206": "Partial Content",			"207": "Multi-Status (WebDAV)",						"300": "Multiple Choices",			"301": "Moved Permanently",			"302": "Found",			"303": "See Other",			"304": "Not Modified",			"305": "Use Proxy",			"307": "Temporary Redirect",						"400": "Bad Request",			"401": "Unauthorized",			"403": "Forbidden",			"404": "Not Found",			"405": "Method Not Allowed",			"406": "Not Acceptable",			"407": "Proxy Authentication Required",			"408": "Request Timeout",			"409": "Conflict",			"410": "Gone",			"411": "Length Required",			"412": "Precondition Failed",			"413": "Request Entity Too Large",			"414": "Request-URI Too Long",			"415": "Unsupported Media Type",			"416": "Requested Range Not Satisfiable",			"417": "Expectation Failed",			"422": "Unprocessable Entity (WebDAV)",			"423": "Locked (WebDAV)",			"424": "Failed Dependency (WebDAV)",			"425": "Unordered Collection",			"426": "Upgrade Required",			"449": "Retry With",			"450": "Blocked",						"500": "Internal Server Error",			"501": "Not Implemented",			"502": "Bad Gateway",			"503": "Service Unavailable",			"504": "Gateway Timeout",			"505": "HTTP Version Not Supported",			"506": "Variant Also Negotiates",			"507": "Insufficient Storage (WebDAV)",			"509": "Bandwidth Limit Exceeded",			"510": "Not Extended"		};						//-----------------------------------------------------------------------------------------		// Public Methods		//-----------------------------------------------------------------------------------------				/**		 * Returns the status message for the specified HTTP Status code.		 * 		 * @param code The HTTP Status code.		 * @return A HTTP Status Message or null if the code has no message.		 */		public static function getStatusCode(code:int):String		{			return _codes[code];		}	}}