	 */		function peek():*;						//-----------------------------------------------------------------------------------------		// Modification Operations		//-----------------------------------------------------------------------------------------				/**		 * Pushes the specified element onto the Stack.		 *		 * @param element The element to push onto the Stack.		 */		function push(element:*):Boolean;						/**		 * Removes and returns the last pushed element from the stack.		 *		 * @return The last pushed element or null if the Stack is empty.		 */		function pop():*;	}}